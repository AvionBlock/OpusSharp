        <file src="runtimes\android-arm64\native\libopus.so" target="runtimes\android-arm64\native"/>
        <file src="runtimes\android-arm\native\libopus.so" target="runtimes\android-arm\native"/>

        <file src="runtimes\linux-x64\native\opus.so" target="runtimes\linux-x64\native"/>
        <file src="runtimes\linux-x86\native\opus.so" target="runtimes\linux-x86\native"/>
        <file src="runtimes\linux-arm64\native\opus.so" target="runtimes\linux-arm64\native"/>
        <file src="runtimes\linux-arm\native\opus.so" target="runtimes\linux-arm\native"/>

        <file src="runtimes\osx-x64\native\opus.dylib" target="runtimes\osx-x64\native"/>
        <file src="runtimes\osx-arm64\native\opus.dylib" target="runtimes\osx-arm64\native"/>

        <file src="./README.md" target="docs"/>
